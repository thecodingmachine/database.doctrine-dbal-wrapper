	},
	"autoload" : {
		"psr-4" : {
			"Mouf\\Doctrine\\DBAL\\" : "src/Mouf/Doctrine/DBAL/"
		}
	},
	"extra" : {
		"mouf" : {
			"install" : [{
					"type" : "url",
					"url" : "dbalconnectioninstall/"
				}
			],
			"logo" : "icons/database.png",
			"require-admin" : [
				"src/DBALConnectionAdmin.php"
			]
		}
	}
}